.nolist 
#include "ti84pce.inc" 
.list 
.org userMem-2 
.db tExtTok,tAsm84CeCmp 
.assume ADL=1 

;-------------------------------
; a few instructions taking 2 cycles:
;	ld a,(hl)	; time: 7
;	adc hl,hl	; time: 8
; a few instructions taking 3 cycles:
;	lea ix,ix+0	; time: 12
;	set 0,(hl)	; time: 15
; a few instructions taking 4 cycles:
;	ld a,(ix+n)	; time: 22
;	ld a,(nn)	; time: 26
;-------------------------------

	di
	ld	hl,$f20030
	ld	a,(hl)
	and	a,%11000000
	ld	(hl),a
	inc	l
	set	1,(hl)
	ld	b,0

_:	push	bc
	ld	hl,$f20000
	ld	de,0
	ld	(hl),e
	inc	l
	ld	(hl),de
	ld	l,$30
	set	0,(hl)
	push	hl

;-------------------------------
	adc	hl,hl
;-------------------------------

	pop	hl
	res	0,(hl)
	ld	l,h
	ld	hl,(hl)
	ld	de,data
	ld	a,(de)
	cp	a,l
	jr	c,+_
	ld	a,l
	ld	(de),a
_:	pop	bc
	djnz	--_
	or	a,a
	sbc	hl,hl
	ld	a,(de)
	ld	l,a
	ld	de,-41
	add	hl,de
	jp	_DispHL
	
data:   .db 255